{
  "manifest_version": 2,
  "name": "HideElement context menu",
  "version": "0.1",
  "description": "Hides elements on the current page, like cookie bars",
  "background": {
    "scripts": ["contextMenu.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["http://*/*", "https://*/*", "contextMenus", "tabs"]
}
