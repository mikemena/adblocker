{
  "name": "Ad Blocker",
  "version": "0",
  "description": "Block annoying ads",
  "manifest_version": 2,

  "icons": {
    "16": "icons/logo_16X16.png",
    "128": "icons/logo_128X128.png"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*", "file:///*/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file:///*/*"],
      "js": ["script.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["style.css"]
}
